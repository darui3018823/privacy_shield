{
    "version": "1.0",
    "domains": {
        "google": {
            "name": "Google Search",
            "matches": [
                "www.google.com",
                "www.google.co.jp"
            ],
            "enabled": true,
            "selectors": [
                ".O4T6Pe",
                ".vqkKIe",
                ".eKPi4",
                "update-location",
                ".dfB0uf"
            ]
        },
        "gemini": {
            "name": "Google Gemini",
            "matches": [
                "gemini.google.com"
            ],
            "enabled": true,
            "selectors": [
                "location-footer",
                ".location-menu-item-container"
            ]
        },
        "amazon": {
            "name": "Amazon.co.jp",
            "matches": [
                "www.amazon.co.jp"
            ],
            "enabled": true,
            "selectors": [
                "#nav-global-location-popover-link",
                "#glow-ingress-block",
                "#contextualIngressPtLink",
                "#contextualIngressPtLabel",
                "#contextualIngressPtLabel_deliveryShortLine",
                ".insert-encrypted-trigger-text"
            ]
        }
    },
    "userRules": {
        "keywords": [],
        "patterns": []
    }
}